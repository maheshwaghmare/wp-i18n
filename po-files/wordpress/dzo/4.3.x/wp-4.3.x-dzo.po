msgstr ""

#: wp-signup.php:714
msgid "The site you were looking for, <strong>%s</strong>, does not exist, but you can create it now!"
msgstr ""

#: wp-signup.php:708
msgid "You are logged in already. No need to register again!"
msgstr "ཁྱོད་ཧེ་མ་ལས་ནང་་་ནུག དོ་རུང་ཐོ་བཀོད་འབད་མི་དགོ "

#: wp-signup.php:706
msgid "Sorry, new registrations are not allowed at this time."
msgstr "དགོངས་མ་ཁྲེལ། ད་ལྟོ་ ཐོ་བཀོད་གསརམ་འབད་མི་མཆོག"

#: wp-signup.php:687
msgid "Site registration has been disabled."
msgstr "ཡོངས་འབྲེལ་འཆར་སྒོ་གསརཔ་ཐོ་བཀོད་འབད་ནི་འདི་ ལྕོགས་མིན་བཟོ་ནུག།"

#: wp-signup.php:681
msgid "User registration has been disabled."
msgstr "ལག་ལེན་པ་ཐོ་བཀོད་འབད་ནི་འདི་ལྕོགས་མིན་བཟོ་ནུག།"

#: wp-signup.php:673
msgid "You must first <a href=\"%s\">log in</a>, and then you can create a new site."
msgstr "ཁྱོད་ཀྱིས་ དང་པ་ར་<a href=\"%s\">ནང་བསྐྱོད་འབད་ནི་</a>དང་ དེ་ལས་ཡོངས་འབྲེལ་འཆར་སྒོ་གསརཔ་བཟོ་ཚུགས།"

#: wp-signup.php:670
msgid "Registration has been disabled."
msgstr "ཐོ་བཀོད་འབད་ནི་འདི་ལྕོགས་མིན་བཟོ་ནུག།"

#: wp-signup.php:664
msgid "Greetings Site Administrator! You are currently allowing &#8220;%s&#8221; registrations. To change or disable registration go to your <a href=\"%s\">Options page</a>."
msgstr "སྐུ་གཟུགས་བཟང་པོ་ ཡོངས་འབྲེལ་བདག་སྐྱོང་པ། ཁྱོད་ཀྱིས་ད་ལྟོ་ &#8220;%s&#8221; ནང་ཐོ་བཀོད་འབད་ཆོགཔ་སྦེ་འདུག འདི་སོར་ནི་ ཡང་ན་ལྕོགས་མིན་འབད་ནིའི་དོན་ལུ་ <a href=\"%s\">གདམ་ཁའི་འཆར་ཤོག</a> ལུ་སོང།"

#: wp-signup.php:661
msgctxt "Multisite active signup type"
msgid "user"
msgstr "ལག་ལེན་པ། ༼སྣ་མང་ཡོངས་འབྲེལ་ཤུགས་ལྡན་ཐོ་བཀོད་དབྱེ་བ།༽"

#: wp-signup.php:660
msgctxt "Multisite active signup type"
msgid "blog"
msgstr "དྲན་དེབ།༼སྣ་མང་་ཡོངས་འབྲེལ་ཤུགས་ལྡན་ཐོ་བཀོད་དབྱེ་བ།༽"

#: wp-signup.php:659
msgctxt "Multisite active signup type"
msgid "none"
msgstr "ག་ཡང་མེནམ། ༼སྣ་མང་་ཡོངས་འབྲེལ་ཤུགས་ལྡན་ཐོ་བཀོད་དབྱེ་བ།༽"

#: wp-signup.php:658
msgctxt "Multisite active signup type"
msgid "all"
msgstr "གེ་ར།/ཆ་ཚང་། ༼སྣ་མང་ཡོངས་འབྲེལ་ཤུགས་ལྡན་ཐོ་བཀོད་དབྱེ་བ།༽"

#: wp-signup.php:637
msgid "Have you entered your email correctly? You have entered %s, if it&#8217;s incorrect, you will not receive your email."
msgstr "ཁྱོད་ཀྱིས་གློག་འཕྲིན་ཁ་བྱང་འདི་མ་འཛོལ་བར་བྲིས་ཡི་ག ཁྱོད་ཀྱིས་ %s འདི་བྲིས་ནུག འདི་འཛོལ་ཏེ་བྲིས་བྲིསཝ་ཨིན་པ་ཅིན་ ང་བཅས་ལས་གློག་འཕྲིན་མི་ལྷོད།"

#: wp-signup.php:636
msgid "Check the junk or spam folder of your email client. Sometime emails wind up there by mistake."
msgstr "ཁྱོད་ཀྱི་གློག་འཕྲིན་གྱི་ མཁོ་མེད(Junk) ཡང་ན་སི་པམ་(Spam) གི་ཡིག་སྣོད་ནང་བལྟ་གནང། འཁྲལ་འཁྲལ་ འཛོལ་ཏེ་ འདི་ནང་འཛུལ་ཏེ་འོང།"

#: wp-signup.php:635
msgid "Wait a little longer. Sometimes delivery of email can be delayed by processes outside of our control."
msgstr "ཨ་ཙི་ཅིག་བསྒུག་གནང། འཕྲལ་འཕྲལ་ ང་བཅས་ཀྱི་བདག་དབང་ལུ་མེད་པའི་གནད་དོན་ལས་བརྟེན་ཏེ་ གློག་འཕྲིན་འདི་ཡུན་འགྱངས་ལུས་འོང།"

#: wp-signup.php:633
msgid "If you haven&#8217;t received your email yet, there are a number of things you can do:"
msgstr "ཁྱོད་ཀྱིས་ད་ལྟོ་ཡང་ གློག་འཕྲིན་འདི་མ་འཐོབ་པར་ཡོད་པ་ཅིན་ འབད་ཐབས་ལེ་ཤ་ཡོད།"

#: wp-signup.php:631
msgid "Still waiting for your email?"
msgstr "ད་ལྟོ་ཡང་གློག་འཕྲིན་ལུ་སྒུགཔ་ཨིན་ན།"

#: wp-signup.php:630
msgid "If you do not activate your site within two days, you will have to sign up again."
msgstr "ཁྱོད་ཀྱིས་ཡོངས་འབྲེལ་འཆར་སྒོ་འདི་ ཉིནམ་གཉིས་ཀྱི་ནང་འཁོད་ལུ་ ཤུགས་ལྡན(activate)མ་བཟོ་བ་ཅིན་ ལོག་སྟེ་ར་ཐོ་བཀོད་འབད་དགོཔ་འཐོན་འོང།"

#: wp-signup.php:628
msgid "But, before you can start using your site, <strong>you must activate it</strong>."
msgstr "ཁྱོད་ཀྱི་ཡོངས་འབྲེལ་འཆར་སྒོ་འདི་ལག་ལེན་འཐབ་ནི་འགོ་མ་བཙུགསཔ་ལས་ ཤུགས་ལྡན་(activate)བཟོ་དགོ།"

#: wp-signup.php:626
msgid "Congratulations! Your new site, %s, is almost ready."
msgstr "བཀྲིས་བདེ་ལེགས། ཁྱོད་ཀྱི་ཡོངས་འབྲེལ་འཆར་སྒོ་གསརཔ་ %s འདི་ཧ་ལམ་གྲ་སྒྲིག་འགྱོ་དོ། "

#: wp-signup.php:565
msgid "Signup"
msgstr "ཐོ་བཀོད་འབད།"

#: wp-signup.php:501
msgid "If you do not activate your username within two days, you will have to sign up again."
msgstr "ཁྱོད་ཀྱི་ལག་ལེན་པའི་མིང་(username)འདི་ ཉིནམ་གཉིས་ཀྱི་ནང་ འཁོད་ནང་ ལག་ལེན་འཐབ་བཏུབ་(activate)མ་བཟོ་བ་ཅིན་ ལོག་སྟེ་ ཐོ་བཀོད་འབད་དགོཔ་འཐོན་འོང།"

#: wp-signup.php:500 wp-signup.php:629
msgid "Check your inbox at <strong>%s</strong> and click the link given."
msgstr ""

#: wp-signup.php:499
msgid "But, before you can start using your new username, <strong>you must activate it</strong>."
msgstr "ཁྱོད་ཀྱི་ལག་ལེན་པའི་མིང་(username)འདི་ལག་ལེན་འཐབ་ནི་འགོ་མ་བཙུགསཔ་ལས་ ཤུགས་ལྡན་(activate)བཟོ་དགོ།"

#: wp-signup.php:498
msgid "%s is your new username"
msgstr "%sའདི་ཁྱོད་ཀྱི་ལག་ལེན་པའི་མིང་(username)གསརཔ་ཨིན།"

#: wp-signup.php:453
msgid "Next"
msgstr "ཤུལ་མམ།"

#: wp-signup.php:449
msgid "Just a username, please."
msgstr "ལག་ལེན་པའི་མིང་(Username)རྐྱངམ་གཅིག་བྲི་གནང།"

#: wp-signup.php:446
msgid "Gimme a site!"
msgstr "ཡོངས་འབྲེལ་གྱི་gimme"

#: wp-signup.php:430
msgid "Get your own %s account in seconds"
msgstr "ཁྱོད་རའི་རྩིས་ཁྲ་%s འདི་སྐར་ཆ་དག་པ་ཅིག་ནང་བཟོ།"

#: wp-signup.php:376
msgid "<a href=\"http://%1$s\">http://%2$s</a> is your new site. <a href=\"%3$s\">Log in</a> as &#8220;%4$s&#8221; using your existing password."
msgstr ""

#: wp-signup.php:374
msgid "The site %s is yours."
msgstr "ཡོངས་འབྲེལ་འཆར་སྒོ་ %s འདི་ཁྱོད་ཀྱི་ཨིན།"

#: wp-signup.php:292
msgid "Create Site"
msgstr "ཡོངས་འབྲེལ་འཆར་སྒོ་བཟོ།"

#: wp-signup.php:277
msgid "If you&#8217;re not going to use a great site domain, leave it for a new user. Now have at it!"
msgstr "ཡོངས་འབྲེལ་འཆར་སྒོའི་ཁ་བྱང་ལེགས་ཤོམ་འདི་ལག་ལེན་མི་འཐབ་ནི་ཨིན་པ་ཅིན་ གཞན་གྱི་དོན་ལུ་བཞག་ད། ད་ལྟོ་ འདི་ལག་ལེན་མཛད་གནང།"

#: wp-signup.php:268
msgid "Sites you are already a member of:"
msgstr "ཁྱོད་ར་ཧེ་མ་ལས་མཐུས་མི་ཨིན་པའི་ ཡོངས་འབྲེལ་འཆར་སྒོའི་ཐོ།"

#: wp-signup.php:262
msgid "Welcome back, %s. By filling out the form below, you can <strong>add another site to your account</strong>. There is no limit to the number of sites you can have, so create to your heart&#8217;s content, but write responsibly!"
msgstr "ལོག་སྟེ་ར་བྱོན་པ་ལེགས་སོ། འབྲི་ཤོག་འདི་བཀང་ཐོག་ལས་ ཁྱོད་ཀྱི་རྩིས་ཁྲའི་ནང་ ཡོངས་འབྲེལ་འཆར་སྒོ་དོ་རུང་ཅིག་བཟོ་ཚུགས། ཡོངས་འབྲེལ་འཆར་སྒོ་བཟོ་ནི་གི་གྱངས་ཁ་ཚད་མེདཔ་ལས་ ཁྱོད་ར་ག་དེ་ཅིག་དགོཔ་ཅིག་བཟོ་ད། ཁྱོད་ཀྱི་མནོ་ཤེས་བསམ་ཤེས་ཀྱི་ཐོག་ལས་ ནང་དོན་(content)ཚུ་འབྲི་ད། དེ་འབདཝ་ད་ གནད་དོན་ཚུ་མནོ་ཤེས་བསམ་ཤེས་ཀྱི་གོ་ལས་བྲི་དགོ།"

#: wp-signup.php:259
msgid "There was a problem, please correct the form below and try again."
msgstr "དཀའ་ངལ་ཅིག་འདུག འོག་གི་འབྲི་ཤོག་ནང་གི་འཛོལ་བ་ཚུ་བཅོས་ཁ་རྐྱབ་སྟེ་ ལོག་སྟེ་ར་བཙུགས་གནང།"

#: wp-signup.php:256
msgid "Get <em>another</em> %s site in seconds"
msgstr "ཡོངས་འབྲེལ་འཆར་སྒོ་སོ་སོ་ཅིག་ སྐར་ཆ་དག་པ་ཅིག་ནང་བཟོ་ད།"

#: wp-signup.php:189
msgid "We send your registration email to this address. (Double-check your email address before continuing.)"
msgstr "ང་བཅས་ཀྱིས་ ཁྱོད་ཀྱི་ཐོ་བཀོད་གློག་འཕྲིན་འདི་ འ་ནི་ཁ་བྱང་ནང་བཏང་ཡི། འཕྲོ་མཐུད་མ་འབད་བའི་ཧེ་མ་ ལོག་སྟེ་རང་གཟིགས་གནང།"

#: wp-signup.php:185
msgid "Email&nbsp;Address:"
msgstr "གློག་འཕྲིན་ཁ་བྱང།"

#: wp-signup.php:182
msgid "(Must be at least 4 characters, letters and numbers only.)"
msgstr "ཉུང་མཐའ་ཡིག་གུ་༤་དགོ། གསལ་བྱེད་དང་ཨང་ཚུ་རྐྱངམ་ཅིག་གི་བཏུབ།"

#: wp-signup.php:135
msgid "No"
msgstr "མེན།"

#: wp-signup.php:131
msgid "Yes"
msgstr "ཨིན།"

