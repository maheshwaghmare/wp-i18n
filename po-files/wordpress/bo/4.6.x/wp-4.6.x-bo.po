#. translators: %s: site address
#: wp-signup.php:885
msgid "The site you were looking for, %s, does not exist."
msgstr "ཁྱེད་ཀྱིས་འཚོལ་བཞིན་པའི་དྲ་ཚིགས %s དེ་མི་འདུག"

#. translators: %s: site address
#: wp-signup.php:880
msgid "The site you were looking for, %s, does not exist, but you can create it now!"
msgstr "ཁྱེད་ཀྱིས་འཚོལ་བཞིན་པའི་དྲ་ཚིགས %s དེ་རྙེད་མ་སོང་། འོན་ཀྱང་ཁྱེད་ཀྱིས་ད་ལྟ་དེ་གསར་བཟོ་བྱ་ཆོག"

#: wp-signup.php:873
msgid "You are logged in already. No need to register again!"
msgstr "ཁྱེད་རང་ནང་འཇུག་བྱས་ཟིན་པས་ཡང་བསྐྱར་ཐོ་འགོད་པའི་ངལ་བ་མ་མཆིས།"

#: wp-signup.php:871
msgid "Sorry, new registrations are not allowed at this time."
msgstr "དགོངས་དག་ཞུ། གནས་སྐབས་སུ་ཐོ་འགོད་ཀྱི་སྒོ་མི་འབྱེད།"

#: wp-signup.php:852
msgid "Site registration has been disabled."
msgstr "དྲ་ཚིགས་ཐོ་འགོད་སྒོ་མི་འབྱེད།"

#: wp-signup.php:846
msgid "User registration has been disabled."
msgstr "བཀོལ་བཀོལ་སྤྱོད་མཁན་ཐོ་འགོད་སྒོ་མི་ཕྱེ།"

#. translators: %s: login URL
#: wp-signup.php:838
msgid "You must first <a href=\"%s\">log in</a>, and then you can create a new site."
msgstr "དྲ་ཚིགས་གསར་བཟོ་བྱ་དགོས་ན་<a href=\"%s\">ནང་འཇུག་</a>བྱ་དགོས།"

#: wp-signup.php:834
msgid "Registration has been disabled."
msgstr "ཐོ་འགོད་བྱ་རུ་འཇུག་གིན་མེད།"

#. translators: 1: type of site sign-up; 2: network settings URL
#: wp-signup.php:827
msgid "Greetings Site Administrator! You are currently allowing &#8220;%s&#8221; registrations. To change or disable registration go to your <a href=\"%s\">Options page</a>."
msgstr "དྲ་ཚིགས་དོ་དམ་པ་ལགས། ཁྱེད་ཀྱིས་མིག་སྔར &#8220;%s&#8221; ཐོ་འགོད་ལ་ཆོག་མཆན་བྱིན་ཡོད། དབང་ཚད་ལ་བཟོ་བཅོས་བྱ་དགོས་ན་ <a href=\"%s\">འདེམས་བྱང་ཤོག་ངོས་</a>སུ་བསྐྱོད་རོགས།"

#: wp-signup.php:823
msgctxt "Multisite active signup type"
msgid "user"
msgstr "སྤྱོད་མཁན།"

#: wp-signup.php:822
msgctxt "Multisite active signup type"
msgid "blog"
msgstr "པོད་ཁུག"

#: wp-signup.php:821
msgctxt "Multisite active signup type"
msgid "none"
msgstr "མེད།"

#: wp-signup.php:820
msgctxt "Multisite active signup type"
msgid "all"
msgstr "ཡོངས།"

#. translators: %s: email address
#: wp-signup.php:764
msgid "Have you entered your email correctly? You have entered %s, if it&#8217;s incorrect, you will not receive your email."
msgstr "ཁྱེད་ཀྱིས་གློག་འཕྲིན་འགྲིག་མིན་ལ་ཞིབ་བསྡུར་བྱ་རོགས། བླུགས་ཡོད་པ་ནི %s རེད་འདུག གལ་ཏེ་མིན་ཚེ་ར་སྤྲོད་ཡི་གེ་འབྱོར་མི་སྲིད།"

#: wp-signup.php:761
msgid "Check the junk or spam folder of your email client. Sometime emails wind up there by mistake."
msgstr "གད་སྙིགས་གློག་འཕྲིན་ནང་དུ་བཙལ་དང་། ལན་རེར་ང་ཚོའི་གློག་འཕྲིན་དུ་ལོག་སྒྲུབ་བྱེད་སྲིད།"

#: wp-signup.php:760
msgid "Wait a little longer. Sometimes delivery of email can be delayed by processes outside of our control."
msgstr "མང་ཙམ་སྒུག་དང་། དྲ་རྒྱའི་རྒྱུ་རྐྱེན་སྣ་ཚོགས་དབང་གིས་གློག་འཕྲིན་གཏོང་བར་འགོར་འགྱངས་བྱུང་སྲིད།"

#: wp-signup.php:758
msgid "If you haven&#8217;t received your email yet, there are a number of things you can do:"
msgstr "གལ་ཏེ་ཁྱེད་ཀྱི་འབྱོར་སྒམ་དུ་སྐུལ་སློང་ཡི་གེ་མ་འབྱོར་ཚེ། ཚོད་ལྟ་བྱ་རུང་བ་ནི:"

#: wp-signup.php:756
msgid "Still waiting for your email?"
msgstr "སྐུལ་སློང་གློག་འཕྲིན་རྙེད་མ་སོང་ངམ?"

#: wp-signup.php:755
msgid "If you do not activate your site within two days, you will have to sign up again."
msgstr "གལ་ཏེ་ཉིན་གཉིས་ཀྱི་ནང་དུ་དྲ་ཚིགས་སྐུལ་སློང་མ་བྱས་ཚེ། ཡང་བསྐྱར་ཐོ་འགོད་བྱ་དགོས།"

#: wp-signup.php:752
msgid "But, before you can start using your site, <strong>you must activate it</strong>."
msgstr "འོན་ཀྱང་དྲ་ཚིགས་མ་སྤྱད་སྔོན་ལ་ངེས་པར་<strong>སྐུལ་སློང་</strong>བྱེད་དགོས།"

#. translators: %s: site address
#: wp-signup.php:750
msgid "Congratulations! Your new site, %s, is almost ready."
msgstr "རྟེན་འབྲེལ་ཞུ། ཁྱེད་ཀྱི་དྲ་ཚིགས་གསར་པ་  \"%s\" ཅུང་ཙམ་གྱིས་སྤྱོད་ཆོག"

#: wp-signup.php:673
msgid "Signup"
msgstr "ཐོ་འགོད།"

#: wp-signup.php:609
msgid "If you do not activate your username within two days, you will have to sign up again."
msgstr "གལ་ཏེ་ཉིན་གཉིས་ཀྱི་ནང་དུ་ཐོ་ཁོངས་སྐུལ་སློང་མ་བྱས་ཚེ། ཡང་བསྐྱར་ཐོ་འགོད་བྱ་དགོས།"

#. translators: %s: email address
#: wp-signup.php:608 wp-signup.php:754
msgid "Check your inbox at %s and click the link given."
msgstr "ཁྱེད་ཀྱི %s གློག་འཕྲིན་འབྱོར་སྒམ་དུ་ཞིབ་བཤེར་བྱས་ནས་མཐུད་སྤྲེལ་མནན་རོགས།"

#: wp-signup.php:606
msgid "But, before you can start using your new username, <strong>you must activate it</strong>."
msgstr "འོན་ཀྱང་ཐོ་ཁོངས་མ་སྤྱད་སྔོན་ལ་<strong>སྐུལ་སློང་</strong>བྱེད་དགོས།"

#. translators: %s: username
#: wp-signup.php:605
msgid "%s is your new username"
msgstr "%s ནི་ཁྱེད་ཀྱི་བཀོལ་མིང་གསར་པ་ཡིན།"

#: wp-signup.php:555
msgid "Just a username, please."
msgstr "བཀོལ་མིང་གཅིག་པུས་འཐུས།"

#: wp-signup.php:552
msgid "Gimme a site!"
msgstr "དུས་མཉམ་དུ་དྲ་ཚིགས་ཤིག་གསར་བཟོ་བྱེད།"

#. translators: %s: name of the network
#: wp-signup.php:535
msgid "Get your own %s account in seconds"
msgstr "སྐར་ཆ་འགའི་ནང་དུ %s ཐོ་ཁོངས་ཤིག་ཐོ་འགོད་ཆོག"

#. translators: 1: home URL, 2: site address, 3: login URL, 4: username
#: wp-signup.php:474
msgid "<a href=\"%1$s\">%2$s</a> is your new site. <a href=\"%3$s\">Log in</a> as &#8220;%4$s&#8221; using your existing password."
msgstr "<a href=“%1$s”>%2$s</a> ནི་ཁྱེད་ཀྱི་དྲ་ཚིགས་གསར་པ་དེ་ཡིན། <a href=“%3$s”>ནང་འཇུག་</a> བྱེད་པར &#8220;%4$s&#8221; ཁྱེད་རང་གི་གསང་གྲངས་དེ་བེད་སྤྱོད་གཏོང་དགོས།"

#. translators: %s: site name
#: wp-signup.php:469
msgid "The site %s is yours."
msgstr "དྲ་ཚིགས %s དེ་ཁྱེད་ལ་དབང་སོང་།"

#: wp-signup.php:343
msgid "Create Site"
msgstr "དྲ་ཚིགས་གསར་བཟོ།"

#: wp-signup.php:328
msgid "If you&#8217;re not going to use a great site domain, leave it for a new user. Now have at it!"
msgstr "མྱུར་ཏུ་མིང་གསར་པ་ཞིག་འདེམས་དང་། མ་གཏོགས་གཞན་པས་འཕྲོག་ན་ཐང།"

#: wp-signup.php:319
msgid "Sites you are already a member of:"
msgstr "ཁྱེད་རང་ཞུགས་པའི་དྲ་ཚིགས:"

#: wp-signup.php:313
msgid "Welcome back, %s. By filling out the form below, you can <strong>add another site to your account</strong>. There is no limit to the number of sites you can have, so create to your heart&#8217;s content, but write responsibly!"
msgstr "%s སྐུ་ཉིད་ཕྱིར་ཕེབས་པར་དགའ་བསུ་ཞུ ཁྱེད་ཀྱིས་ <strong>ཡང་བསྐྱར་རང་གི་ཐོ་ཁོངས་ཁྲོད་དུ་དྲ་ཚིགས་གསར་སྣོན་</strong> བྱ་ཆོག ང་ཚོས་བཀོལ་སྤྱོད་པའི་ཡོད་ཆོག་པའི་དྲ་ཚིགས་བསྡོམས་འབོར་ལ་ཚོད་འཛིན་མི་བྱེད། དེ་བས་ཁྱེད་ལ་རྩོམ་འབྲིའི་འཆར་སྒོ་འདང་ངེས་ཤིག་ཡོད་ན། ང་ཚོས་ཁྱེད་ཀྱིས་དྲ་ཚིགས་གསར་བ་བཟོ་བར་དགའ་བསུ་བྱེད།"

#: wp-signup.php:310
msgid "There was a problem, please correct the form below and try again."
msgstr "རེའུ་མིག་ནང་དུ་ནོར་འཁྲུལ་འདུག་པས་ནོར་བཅོས་བྱས་རྗེས་ཡང་བསྐྱར་ཚོད་ལྟ་བྱ་རོགས།"

#: wp-signup.php:307
msgid "Get <em>another</em> %s site in seconds"
msgstr "སྐར་ཆ་འགའི་ནང་དུ་སླར་ཡང་དྲ་ཚིགས %s གཅིག་<strong>ཐོ་འགོད་</strong>བྱ་ཆོག"

#: wp-signup.php:240
msgid "We send your registration email to this address. (Double-check your email address before continuing.)"
msgstr "ང་ཚོས་གློག་འཕྲིན་འདིའི་ནང་ངོས་ལེན་ཡི་གེ་བསྐུར་རྒྱུ་ཡིན་པས་འགྲིག་མིན་ལ་ཞིབ་ལྟ་བྱོས།"

#: wp-signup.php:236
msgid "Email&nbsp;Address:"
msgstr "གློག་འཕྲིན："

#: wp-signup.php:233
msgid "(Must be at least 4 characters, letters and numbers only.)"
msgstr "(ཉུང་མཐར་ཡིག་རྟགས ༤ དགོས་པ་དང་། ཨང་ཀི་དང་དབྱིན་ཡིག་གསལ་བྱེད་ཁོ་ན་སྤྱོད་རུང་།)"

#: wp-signup.php:182
msgid "No"
msgstr "མིན།"

#: wp-signup.php:178
msgid "Yes"
msgstr "ཡིན།"

#: wp-signup.php:174
msgid "Allow search engines to index this site."
msgstr "འཚོལ་བཤེར་མ་ལག་གིས་དྲ་ཚིགས་འདི་ལ་འཚོལ་བཤེར་སྒྲིག་ཆོག"

#: wp-signup.php:173
msgid "Privacy:"
msgstr "གསང་དོན:"

#: wp-signup.php:146
msgid "Site Language:"
msgstr "དྲ་ཚིགས་ཀྱི་སྐད་ཡིག:"

#: wp-signup.php:132
msgid "Site Title:"
msgstr "དྲ་ཚིགས་ཀྱི་ཁ་བྱང:"

#: wp-signup.php:127
msgid "Must be at least 4 characters, letters and numbers only. It cannot be changed, so choose carefully!"
msgstr "ཉུང་མཐར་ཡིག་རྟགས 4 དགོས་པ་དང་། ཨང་ཀི་དང་དབྱིན་ཡིག་གསལ་བྱེད་ཁོ་ན་སྤྱོད་རུང་། གཏན་ལ་འཁེལ་རྗེས་བཟོ་བཅོས་བྱ་མི་ཐུབ།"

#. translators: %s: site address
#: wp-signup.php:127
msgid "Your address will be %s."
msgstr "ཁྱེད་ཀྱི་ས་གནས་ནི %s。"

#: wp-signup.php:123
msgid "domain"
msgstr "ཁོངས་མིང་།"

#: wp-signup.php:121
